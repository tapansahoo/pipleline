                echo 'doing build ' 
            }
        }

        stage('Test') {
            steps {
                echo 'doing  Test' 
            }
        }
        
        stage('deploy') {
            steps {
                echo 'doing  deploy   now' 
            }
        }
    
       
    
    }
}
